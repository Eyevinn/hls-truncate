  "description": "NPM library to truncate an HLS VOD manifest into a new VOD manifest",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --project ./",
    "test": "jasmine-ts",
    "prepare": "husky install",
    "lint": "eslint .",
    "pretty": "prettier --check --ignore-unknown .",
    "pretty:format": "prettier --write --ignore-unknown .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "coverage": "nyc npm test && nyc report",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eyevinn/hls-truncate.git"
  },
  "keywords": [
    "hls",
    "m3u8",
    "slate"
  ],
  "author": "Eyevinn Technology <work@eyevinn.se>",
  "contributors": [
    "Jonas Birm√© <jonas.birme@eyevinn.se> (Eyevinn Technology AB)",
    "Alan Allard <alan.allard@eyevinn.se> (Eyevinn Technology AB)",
    "Nicholas Frederiksen <nicholas.frederiksen@eyevinn.se> (Eyevinn Technology AB)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eyevinn/hls-truncate/issues"
  },
  "homepage": "https://github.com/Eyevinn/hls-truncate#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jasmine": "^4.3.3",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "coveralls": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine": "^3.6.4",
    "jasmine-ts": "^0.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@eyevinn/m3u8": "0.5.3",
    "node-fetch": "2.6.2"
  }
